[
  {
    "description": "这是一个带header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "forms": {
        "name": "wanglaosi",
        "sex": "man"
      }
    },
    "response": {
      "json": {
        "status": "1",
        "wanglaosi": "success"
      }
    }
  }
]